    "path": "^0.12.7",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.3.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
