  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^18.7.23",
    "@typescript-eslint/parser": "^5.17.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^4.1.0",
